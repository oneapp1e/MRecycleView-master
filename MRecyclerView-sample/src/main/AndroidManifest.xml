<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mlr.demo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name="com.mlr.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
    >
        <activity android:name="com.mlr.demo.SplashActivity"
        >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.mlr.demo.MainActivity"
                  android:launchMode="singleTask"
        />

        <activity android:name="com.mlr.demo.CommonListActivity"/>
        <activity android:name="com.mlr.demo.HorizontalCommonListActivity"/>
        <activity android:name="com.mlr.demo.CommonGridActivity"/>
        <activity android:name="com.mlr.demo.HorizontalCommonGridActivity"/>
        <activity android:name="com.mlr.demo.MixListGridActivity"/>
        <activity android:name="com.mlr.demo.SectionListActivity"/>
        <activity android:name="com.mlr.demo.SectionGridActivity"/>
        <activity android:name="com.mlr.demo.DragMoveListActivity"/>
        <activity android:name="com.mlr.demo.DragMoveGridActivity"/>
        <activity android:name="com.mlr.demo.PullToRefreshListActivity"/>
        <activity android:name="com.mlr.demo.SwipeRefreshListActivity"/>
        <activity android:name="com.mlr.demo.AnimationListActivity"/>
        <activity android:name="com.mlr.demo.Retrofit2Activity"/>


        <meta-data
            android:name="com.taobao.android.hotfix.IDSECRET"
            android:value="72165-1"/>
        <meta-data
            android:name="com.taobao.android.hotfix.APPSECRET"
            android:value="d98c111396f2a6d03e7d944d8d1ff40a"/>
        <meta-data
            android:name="com.taobao.android.hotfix.RSASECRET"
            android:value=""/>
